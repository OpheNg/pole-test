<div class="createbox">


<h2>session</h2>


  <div class="createform">

    <p class="createsession">
      <%= simple_form_for [@session] do |f| %>
        <%= f.input :date %>
        <%= f.input :start_time %>
        <%= f.input :end_time %>
        <%= f.input :address %>
        <% f.input :user_id, input_html: {value: params[:user_id]} %>
        <%= f.button :submit %>
      <% end %>
    </p>
  </div>



</div>
